    "def kadane(A):\n",
    "    max_current , max_global = A[0] , A[0]\n",
    "    for i in range(1,len(A)):\n",
    "        max_current=max(A[i],max_current+A[i])\n",
    "        if max_current>max_global:\n",
    "            max_global=max_current    \n",
    "    return max_global\n",
    "\n",
    "kadane([-2,1,-3,4,-1,2,1,-5,4])"
   ]
